<?xml version="1.0" encoding="UTF-8"?>
<Element xmlns="urn:fdc:difi.no:2017:vefa:structure-1" cardinality="0..1">
    
        <Term>cac:InvoicePeriod</Term>
        <Name>INVOICE LINE PERIOD</Name>
        <Description>A group of business terms providing information about the period relevant for the Invoice line.
        </Description>
        <Reference type="BUSINESS_TERM">BG-26</Reference>
        
        <Element cardinality="0..1">
            <Term>cbc:StartDate</Term>
            <Name>Invoice line period start date</Name>
            <Description>The date when the Invoice period for this Invoice line starts. Format ="YYYY-MM-DD"</Description>
            <DataType>Date</DataType>
            <Reference type="BUSINESS_TERM">BT-134</Reference>
            <Reference type="RULE">BR-CO-20</Reference>
            <Reference type="RULE">PEPPOL-EN16931-R110</Reference>
            <Reference type="RULE">PEPPOL-EN16931-F001</Reference>
            <Value type="EXAMPLE">2017-10-05</Value>
        </Element>
        <Element cardinality="0..1">
            <Term>cbc:EndDate</Term>
            <Name>Invoice line period end date</Name>
            <Description>The date when the Invoice period for this Invoice line ends. Format ="YYYY-MM-DD"</Description>
            <DataType>Date</DataType>
            <Reference type="BUSINESS_TERM">BT-135</Reference>
            <Reference type="RULE">BR-30</Reference>
            <Reference type="RULE">BR-CO-20</Reference>
            <Reference type="RULE">PEPPOL-EN16931-R111</Reference>
            <Reference type="RULE">PEPPOL-EN16931-F001</Reference>
            <Value type="EXAMPLE">2017-10-15</Value>
        </Element>
    </Element>