<?xml version="1.0" encoding="UTF-8"?>
<testSet xmlns="http://digst.dk/xsd/validator/1.0" configuration="peppolbis-en16931-base-3.0">
	<assert>
		<description>For Danish suppliers when the Accounting is known it should be referred on the Invoice</description>
		<scope>DK-R-001</scope>
	</assert>
	<test>
		<assert>
			<success>DK-R-001</success>
		</assert>
		<Invoice xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" xmlns="urn:oasis:names:specification:ubl:schema:xsd:Invoice-2">
			<cbc:AccountingCost>5250124502</cbc:AccountingCost>
			<cac:AccountingSupplierParty>
				<cac:Party>
					<cac:PostalAddress>
						<cac:Country>
							<cbc:IdentificationCode>DK</cbc:IdentificationCode>
						</cac:Country>
					</cac:PostalAddress>
				</cac:Party>
			</cac:AccountingSupplierParty>
		</Invoice>
	</test>
	
	<test>
		<assert>
			<success>DK-R-001</success>
		</assert>
		<Invoice xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" xmlns="urn:oasis:names:specification:ubl:schema:xsd:Invoice-2">
			<cbc:AccountingCost></cbc:AccountingCost>
			<cac:AccountingSupplierParty>
				<cac:Party>
					<cac:PostalAddress>
						<cac:Country>
							<cbc:IdentificationCode>NO</cbc:IdentificationCode>
						</cac:Country>
					</cac:PostalAddress>
				</cac:Party>
			</cac:AccountingSupplierParty>
		</Invoice>
	</test>	
	
	<test>
		<assert>
			<warning>DK-R-001</warning>
		</assert>
		<Invoice xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" xmlns="urn:oasis:names:specification:ubl:schema:xsd:Invoice-2">
			<cbc:AccountingCost></cbc:AccountingCost>
			<cac:AccountingSupplierParty>
				<cac:Party>
					<cac:PostalAddress>
						<cac:Country>
							<cbc:IdentificationCode>DK</cbc:IdentificationCode>
						</cac:Country>
					</cac:PostalAddress>
				</cac:Party>
			</cac:AccountingSupplierParty>
		</Invoice>
	</test>

	
	<test>
		<assert>
			<warning>DK-R-001</warning>
		</assert>
		<Invoice xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" xmlns="urn:oasis:names:specification:ubl:schema:xsd:Invoice-2">
<!--			<cbc:AccountingCost></cbc:AccountingCost>
-->			<cac:AccountingSupplierParty>
				<cac:Party>
					<cac:PostalAddress>
						<cac:Country>
							<cbc:IdentificationCode>DK</cbc:IdentificationCode>
						</cac:Country>
					</cac:PostalAddress>
				</cac:Party>
			</cac:AccountingSupplierParty>
		</Invoice>
	</test>
	
	

</testSet>
